aff55123a98c"],

Cell[BoxData[
 RowBox[{
  RowBox[{"newtonRawson", " ", "=", " ", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "f", ",", " ", "around", ",", " ", "iterationsLimit", ",", " ", 
       "minInterval", ",", " ", "precision"}], "}"}], ",", "\n", "\t", 
     RowBox[{
      RowBox[{"root", " ", "=", " ", "around"}], ";", "\n", "\t", 
      RowBox[{"approximations", " ", "=", " ", 
       RowBox[{"{", "}"}]}], ";", "\n", "\t", 
      RowBox[{"slopes", " ", "=", " ", 
       RowBox[{"{", "}"}]}], ";", "\n", "\t", 
      RowBox[{"Begin", "[", "\"\<newtonRawson`\>\"", "]"}], ";", " ", "\n", 
      "\t", 
      RowBox[{"midValue", " ", "=", " ", 
       RowBox[{"f", "[", "root", "]"}]}], ";", " ", "\n", "\t", 
      RowBox[{"width", " ", "=", " ", "minInterval"}], ";", "\n", "\t", 
      RowBox[{"df", " ", "=", " ", 
       RowBox[{
        RowBox[{"Derivative", "[", "1", "]"}], "[", "f", "]"}]}], ";", "\n", 
      "\t", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "0"}], ",", " ", 
        RowBox[{
         RowBox[{"i", " ", "<", " ", "iterationsLimit"}], " ", "&&", " ", 
         RowBox[{
          RowBox[{"Abs", "[", "width", "]"}], " ", ">=", " ", "minInterval"}],
          " ", "&&", " ", 
         RowBox[{
          RowBox[{"Abs", "[", "midValue", "]"}], " ", ">=", " ", 
          "precision"}]}], ",", " ", 
        RowBox[{"++", "i"}], ",", "\n", "\t\t", 
        RowBox[{
         RowBox[{"slope", " ", "=", " ", 
          RowBox[{"df", "[", "root", "]"}]}], ";", "\n", "\t\t", 
         RowBox[{"newRoot", " ", "=", " ", 
          RowBox[{"root", " ", "-", " ", 
           FractionBox["midValue", "slope"]}]}], ";", "\n", "\t\t", 
         RowBox[{"width", " ", "=", " ", 
          RowBox[{"Abs", "[", 
           RowBox[{"newRoot", " ", "-", " ", "root"}], "]"}]}], ";", "\n", 
         "\t\t", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"approximations", ",", " ", 
           RowBox[{"{", 
            RowBox[{"root", ",", " ", "newRoot"}], "}"}]}], "]"}], ";", "\n", 
         "\t\t", 
         RowBox[{"root", " ", "=", " ", "newRoot"}], ";", "\n", "\t\t", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"slopes", ",", " ", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"1", ",", " ", "slope"}], "}"}], ",", " ", 
             RowBox[{"{", 
              RowBox[{"0", ",", " ", "1"}], "}"}]}], "}"}]}], "]"}], ";", 
         "\n", "\t\t", 
         RowBox[{"midValue", " ", "=", " ", 
          RowBox[{"N", "[", 
           RowBox[{"f", "[", "root", "]"}], "]"}]}], ";"}]}], "\n", "\t", 
       "]"}], ";", "\n", "\t", 
      RowBox[{"End", "[", "]"}], ";", "\n", "\t", 
      RowBox[{"{", 
       RowBox[{"root", ",", " ", "approximations", ",", " ", "slopes"}], 
       "}"}]}]}], "\n", "]"}]}], ";"}]], "Code",
