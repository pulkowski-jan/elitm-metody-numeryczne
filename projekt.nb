Cell[BoxData[
 RowBox[{
  RowBox[{"riemann", " ", "=", " ", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "f", ",", " ", "intervalStart", ",", " ", "intervalEnd", ",", " ", 
       "slicesCount", ",", " ", "selector"}], "}"}], ",", "\n", "\t", 
     RowBox[{
      RowBox[{"riemannSum", " ", "=", " ", "0."}], ";", "\n", "\t", 
      RowBox[{"Begin", "[", "\"\<riemman`\>\"", "]"}], ";", "\n", "\t", 
      RowBox[{"direction", " ", "=", " ", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"intervalStart", " ", "<", " ", "intervalEnd"}], ",", " ", 
         "1", ",", " ", 
         RowBox[{"-", "1"}]}], "]"}]}], ";", "\n", "\t", 
      RowBox[{"start", " ", "=", " ", 
       RowBox[{"Min", "[", 
        RowBox[{"intervalStart", ",", " ", "intervalEnd"}], "]"}]}], ";", 
      "\n", "\t", 
      RowBox[{"len", "=", 
       FractionBox[
        RowBox[{"Abs", "[", 
         RowBox[{"intervalEnd", "-", "intervalStart"}], "]"}], 
        "slicesCount"]}], ";", "\n", "\t", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "0"}], ",", " ", 
        RowBox[{"i", " ", "<", " ", "slicesCount"}], ",", " ", 
        RowBox[{"++", "i"}], ",", "\n", "\t\t", 
        RowBox[{
         RowBox[{"end", " ", "=", " ", 
          RowBox[{"start", " ", "+", " ", "len"}]}], ";", "\n", "\t\t", 
         RowBox[{"c", " ", "=", " ", 
          RowBox[{"selector", "[", 
           RowBox[{"start", ",", " ", "end"}], "]"}]}], ";", "\n", "\t\t", 
         RowBox[{"riemannSum", " ", "+=", " ", 
          RowBox[{
           RowBox[{"f", "[", "c", "]"}], " ", "*", " ", 
           RowBox[{"Abs", "[", 
            RowBox[{"end", " ", "-", " ", "start"}], "]"}]}]}], ";", "\n", 
         "\t\t", 
         RowBox[{"start", " ", "=", " ", "end"}], ";"}]}], "\n", "\t", "]"}], 
      ";", "\n", "\t", 
      RowBox[{"End", "[", "]"}], ";", "\n", "\t", 
      RowBox[{"direction", " ", "*", " ", "riemannSum"}]}]}], "\n", "]"}]}], 
  ";"}]], "Code",
