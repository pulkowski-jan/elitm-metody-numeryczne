Cell[BoxData[
 RowBox[{
  RowBox[{"visualMonteCarlo", " ", "=", " ", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "fun", ",", " ", "intervalStart", ",", " ", "intervalEnd", ",", " ", 
       "sampleSize"}], "}"}], ",", "\n", "\t", 
     RowBox[{
      RowBox[{"samplesIn", " ", "=", " ", "0"}], ";", "\n", "\t", 
      RowBox[{
       RowBox[{"f", "[", "x_", "]"}], ":=", 
       RowBox[{"N", "[", 
        RowBox[{"fun", "[", "x", "]"}], "]"}]}], ";", "\n", "\t", 
      RowBox[{"direction", " ", "=", " ", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"intervalStart", " ", "<", " ", "intervalEnd"}], ",", " ", 
         "1", ",", " ", 
         RowBox[{"-", "1"}]}], "]"}]}], ";", "\n", "\t", 
      RowBox[{"start", " ", "=", " ", 
       RowBox[{"Min", "[", 
        RowBox[{"intervalStart", ",", " ", "intervalEnd"}], "]"}]}], ";", 
      "\n", "\t", 
      RowBox[{"end", " ", "=", " ", 
       RowBox[{"Max", "[", 
        RowBox[{"intervalStart", ",", " ", "intervalEnd"}], "]"}]}], ";", 
      "\n", "\t", 
      RowBox[{"min", " ", "=", " ", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"Minimize", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"f", "[", "z", "]"}], ",", " ", 
              RowBox[{"start", " ", "<=", " ", "z", " ", "<=", " ", "end"}]}],
              "}"}], ",", " ", 
            RowBox[{"{", "z", "}"}]}], "]"}], "[", 
          RowBox[{"[", "2", "]"}], "]"}], "[", 
         RowBox[{"[", "1", "]"}], "]"}], "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\n", "\t", 
      RowBox[{"max", " ", "=", " ", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"Maximize", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"f", "[", "z", "]"}], ",", " ", 
              RowBox[{"start", " ", "<=", " ", "z", " ", "<=", " ", "end"}]}],
              "}"}], ",", " ", 
            RowBox[{"{", "z", "}"}]}], "]"}], "[", 
          RowBox[{"[", "2", "]"}], "]"}], "[", 
         RowBox[{"[", "1", "]"}], "]"}], "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\n", "\t", 
      RowBox[{"areaSize", " ", "=", " ", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"max", " ", "-", " ", "min"}], ")"}], " ", "*", " ", 
        RowBox[{"(", 
         RowBox[{"end", " ", "-", " ", "start"}], ")"}]}]}], ";", "\n", "\t", 
      
      RowBox[{"points", " ", "=", " ", 
       RowBox[{"{", "}"}]}], ";", "\n", "\t", 
      RowBox[{"results", " ", "=", " ", 
       RowBox[{"{", "}"}]}], ";", "\n", "\t", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "0"}], ",", " ", 
        RowBox[{"i", " ", "<", " ", "sampleSize"}], ",", " ", 
        RowBox[{"++", "i"}], ",", " ", "\n", "\t\t", 
        RowBox[{
         RowBox[{"x", " ", "=", " ", 
          RowBox[{"RandomReal", "[", 
           RowBox[{"{", 
            RowBox[{"start", ",", " ", "end"}], "}"}], "]"}]}], ";", "\n", 
         "\t\t", 
         RowBox[{"y", " ", "=", " ", 
          RowBox[{"RandomReal", "[", 
           RowBox[{"{", 
            RowBox[{"min", ",", " ", "max"}], "}"}], "]"}]}], ";", "\n", 
         "\t\t", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Abs", "[", "y", "]"}], " ", "<=", " ", 
            RowBox[{"Abs", "[", 
             RowBox[{"f", "[", "x", "]"}], "]"}]}], ",", " ", 
           RowBox[{
            RowBox[{"++", "samplesIn"}], ";", " ", 
            RowBox[{"AppendTo", "[", 
             RowBox[{"points", ",", " ", "Red"}], "]"}]}], ",", " ", 
           RowBox[{"AppendTo", "[", 
            RowBox[{"points", ",", " ", "Blue"}], "]"}]}], "]"}], ";", "\n", 
         "\t\t", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"points", ",", " ", 
           RowBox[{"Point", "[", 
            RowBox[{"{", 
             RowBox[{"x", ",", " ", "y"}], "}"}], "]"}]}], "]"}], ";", "\n", 
         "\t\t", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"results", ",", " ", 
           RowBox[{"direction", " ", "*", " ", "areaSize", " ", "*", " ", 
            FractionBox["samplesIn", "sampleSize"]}]}], "]"}], ";"}]}], "\n", 
       "\t", "]"}], ";", "\n", "\t", 
      RowBox[{"Animate", "[", "\n", "\t\t", 
       RowBox[{
        RowBox[{"Plot", "[", 
         RowBox[{
          RowBox[{"fun", "[", "z", "]"}], ",", " ", 
          RowBox[{"{", 
           RowBox[{"z", ",", " ", 
            RowBox[{"intervalStart", " ", "-", " ", "0.1"}], ",", " ", 
            RowBox[{"intervalEnd", " ", "+", " ", "0.1"}]}], "}"}], ",", " ", 
          
          RowBox[{"PlotRange", "->", "Full"}], ",", " ", 
          RowBox[{"PlotLabel", "->", 
           RowBox[{"StringJoin", "[", 
            RowBox[{"\"\<Area = \>\"", ",", " ", 
             RowBox[{"ToString", "[", 
              RowBox[{"results", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "]"}]}], "]"}]}], ",", " ", 
          RowBox[{"Epilog", "->", 
           RowBox[{"points", "[", 
            RowBox[{"[", 
             RowBox[{"1", " ", ";;", " ", "i"}], "]"}], "]"}]}]}], "]"}], ",",
         "\n", "\t\t", 
        RowBox[{"{", 
         RowBox[{"i", ",", " ", "0", ",", " ", "sampleSize", ",", " ", "1"}], 
         "}"}], ",", " ", 
        RowBox[{"AnimationRepetitions", " ", "->", " ", "1"}]}], "\n", "\t", 
       "]"}]}]}], "\n", "]"}]}], ";"}]], "Code",
