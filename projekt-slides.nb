Cell[BoxData[
 RowBox[{
  RowBox[{"bisect", " ", "=", " ", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "f", ",", " ", "intervalStart", ",", " ", "intervalEnd", ",", " ", 
       "iterationsLimit", ",", " ", "minInterval", ",", " ", "precision"}], 
      "}"}], ",", " ", "\n", "\t", 
     RowBox[{
      RowBox[{"Clear", "[", 
       RowBox[{"root", ",", " ", "bounds"}], "]"}], ";", "\n", "\t", 
      RowBox[{"root", " ", "=", " ", 
       RowBox[{"intervalStart", " ", "+", " ", 
        FractionBox[
         RowBox[{"intervalEnd", " ", "-", " ", "intervalStart"}], "2"]}]}], 
      ";", "\n", "\t", 
      RowBox[{"bounds", " ", "=", " ", 
       RowBox[{"{", 
        RowBox[{"{", 
         RowBox[{"intervalStart", ",", " ", "intervalEnd"}], "}"}], "}"}]}], 
      ";", "\n", "\t", 
      RowBox[{"Begin", "[", "\"\<bisect`\>\"", "]"}], ";", " ", "\n", "\t", 
      RowBox[{"start", " ", "=", " ", "intervalStart"}], ";", "\n", "\t", 
      RowBox[{"end", " ", "=", " ", "intervalEnd"}], ";", "\n", "\t", 
      RowBox[{"valueAtStart", " ", "=", " ", 
       RowBox[{"f", "[", "start", "]"}]}], ";", "\n", "\t", 
      RowBox[{"valueAtEnd", " ", "=", " ", 
       RowBox[{"f", "[", "end", "]"}]}], ";", "\n", "\t", 
      RowBox[{"width", " ", "=", " ", 
       RowBox[{"end", " ", "-", " ", "start"}]}], ";", "\n", "\t", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Sign", "[", "valueAtStart", "]"}], " ", "!=", " ", 
         RowBox[{"Sign", "[", "valueAtEnd", "]"}]}], ",", " ", "\n", "\t\t", 
        RowBox[{
         RowBox[{"width", " ", "/=", " ", "2"}], ";", "\n", "\t\t", 
         RowBox[{"mid", " ", "=", " ", 
          RowBox[{"start", " ", "+", " ", "width"}]}], ";", "\n", "\t\t", 
         RowBox[{"midValue", " ", "=", " ", 
          RowBox[{"f", "[", "mid", "]"}]}], ";", "\n", "\t\t", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", " ", "=", " ", "0"}], ",", " ", 
           RowBox[{
            RowBox[{"i", " ", "<", " ", "iterationsLimit"}], " ", "&&", " ", 
            RowBox[{
             RowBox[{"Abs", "[", "width", "]"}], " ", ">=", " ", 
             "minInterval"}], " ", "&&", " ", 
            RowBox[{
             RowBox[{"Abs", "[", "midValue", "]"}], " ", ">=", " ", 
             "precision"}]}], ",", " ", 
           RowBox[{"++", "i"}], ",", "\n", "\t\t\t", 
           RowBox[{
            RowBox[{"root", " ", "=", " ", "mid"}], ";", "\n", "\t\t\t", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"Sign", "[", "valueAtStart", "]"}], " ", "!=", " ", 
               RowBox[{"Sign", "[", "midValue", "]"}]}], ",", " ", "\n", 
              "\t\t\t\t", 
              RowBox[{
               RowBox[{"end", " ", "=", " ", "mid"}], ";", "\n", "\t\t\t\t", 
               RowBox[{"valueAtEnd", " ", "=", " ", "midValue"}], ";"}], "\n",
               "\t\t\t\t", ",", " ", 
              RowBox[{"(*", " ", "else", " ", "*)"}], "\n", "\t\t\t\t", 
              RowBox[{
               RowBox[{"start", " ", "=", " ", "mid"}], ";", "\n", "\t\t\t\t", 
               RowBox[{"valueAtStart", " ", "=", " ", "midValue"}], ";"}]}], 
             "\n", "\t\t\t", "]"}], ";", "\n", "\t\t\t", 
            RowBox[{"AppendTo", "[", 
             RowBox[{"bounds", ",", " ", 
              RowBox[{"{", 
               RowBox[{"start", ",", " ", "end"}], "}"}]}], "]"}], ";", "\n", 
            "\t\t\t", 
            RowBox[{"width", " ", "/=", " ", "2"}], ";", "\n", "\t\t\t", 
            RowBox[{"mid", " ", "=", " ", 
             RowBox[{"start", " ", "+", " ", "width"}]}], ";", "\n", "\t\t\t", 
            RowBox[{"midValue", " ", "=", " ", 
             RowBox[{"f", "[", "mid", "]"}]}], ";"}]}], "\n", "\t\t", "]"}], 
         ";", "\n", "\t\t", 
         RowBox[{"root", " ", "=", " ", "mid"}], ";"}]}], "\n", "\t", "]"}], 
      ";", "\n", "\t", 
      RowBox[{"End", "[", "]"}], ";", "\n", "\t", 
      RowBox[{"{", 
       RowBox[{"root", ",", " ", "bounds"}], "}"}]}]}], "\n", "]"}]}], 
  ";"}]], "Code",
