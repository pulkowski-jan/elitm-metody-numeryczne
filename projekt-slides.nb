Cell[BoxData[
 RowBox[{
  RowBox[{"falsi", " ", "=", " ", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "fun", ",", " ", "intervalStart", ",", " ", "intervalEnd", ",", " ", 
       "iterationsLimit", ",", " ", "minInterval", ",", " ", "precision"}], 
      "}"}], ",", " ", "\n", "\t", 
     RowBox[{
      RowBox[{"Clear", "[", 
       RowBox[{"root", ",", " ", "bounds"}], "]"}], ";", "\n", "\t", 
      RowBox[{"bounds", " ", "=", " ", 
       RowBox[{"{", "}"}]}], ";", "\n", "\t", 
      RowBox[{"slopes", " ", "=", " ", 
       RowBox[{"{", "}"}]}], ";", "\n", "\t", 
      RowBox[{"root", " ", "=", " ", "intervalEnd"}], ";", "\n", "\t", 
      RowBox[{"Begin", "[", "\"\<falsi`\>\"", "]"}], ";", " ", "\n", "\t", 
      RowBox[{
       RowBox[{"f", "[", "x_", "]"}], ":=", 
       RowBox[{"N", "[", 
        RowBox[{"fun", "[", "x", "]"}], "]"}]}], ";", "\n", "\t", 
      RowBox[{"start", " ", "=", " ", "intervalStart"}], ";", "\n", "\t", 
      RowBox[{"end", " ", "=", " ", "intervalEnd"}], ";", "\n", "\t", 
      RowBox[{"valueAtStart", " ", "=", " ", 
       RowBox[{"f", "[", "start", "]"}]}], ";", "\n", "\t", 
      RowBox[{"valueAtEnd", " ", "=", " ", 
       RowBox[{"f", "[", "end", "]"}]}], ";", "\n", "\t", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Sign", "[", "valueAtStart", "]"}], " ", "!=", " ", 
         RowBox[{"Sign", "[", "valueAtEnd", "]"}]}], ",", " ", "\n", "\t\t", 
        RowBox[{
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", "=", "0"}], ",", " ", 
           RowBox[{
            RowBox[{"i", " ", "<", " ", "iterationsLimit"}], " ", "&&", " ", 
            RowBox[{
             RowBox[{"Abs", "[", 
              RowBox[{"end", " ", "-", " ", "start"}], "]"}], " ", ">=", " ", 
             "minInterval"}], " ", "&&", " ", 
            RowBox[{
             RowBox[{"Abs", "[", 
              RowBox[{"f", "[", "root", "]"}], "]"}], " ", ">=", " ", 
             "precision"}]}], ",", " ", 
           RowBox[{"++", "i"}], ",", "\n", "\t\t\t", 
           RowBox[{
            RowBox[{"a", " ", "=", " ", 
             FractionBox[
              RowBox[{"valueAtEnd", " ", "-", " ", "valueAtStart"}], 
              RowBox[{"end", " ", "-", " ", "start"}]]}], ";", "\n", "\t\t\t", 
            RowBox[{"root", " ", "=", " ", 
             RowBox[{"end", " ", "-", " ", 
              FractionBox["valueAtEnd", "a"]}]}], ";", "\n", "\t\t\t", 
            RowBox[{"AppendTo", "[", 
             RowBox[{"bounds", ",", " ", 
              RowBox[{"{", 
               RowBox[{"start", ",", " ", "end", ",", " ", "root"}], "}"}]}], 
             "]"}], ";", "\n", "\t\t\t", 
            RowBox[{"AppendTo", "[", 
             RowBox[{"slopes", ",", " ", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"end", " ", "-", " ", "start"}], ",", " ", 
                  RowBox[{"valueAtEnd", " ", "-", " ", "valueAtStart"}]}], 
                 "}"}], ",", " ", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"end", " ", "-", " ", "start"}], ",", " ", 
                  RowBox[{"valueAtEnd", " ", "-", " ", "valueAtStart"}]}], 
                 "}"}], ",", " ", 
                RowBox[{"{", 
                 RowBox[{"0", ",", " ", "1"}], "}"}]}], "}"}]}], "]"}], ";", 
            "\n", "\t\t\t", 
            RowBox[{"rootValue", " ", "=", " ", 
             RowBox[{"f", "[", "root", "]"}]}], ";", "\n", "\t\t\t", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"Sign", "[", "valueAtStart", "]"}], " ", "!=", " ", 
               RowBox[{"Sign", "[", "rootValue", "]"}]}], ",", " ", "\n", 
              "\t\t\t\t", 
              RowBox[{
               RowBox[{"end", " ", "=", " ", "root"}], ";", "\n", "\t\t\t\t", 
               
               RowBox[{"valueAtEnd", " ", "=", " ", "rootValue"}], ";"}], 
              "\n", "\t\t\t\t", ",", "\n", "\t\t\t\t", 
              RowBox[{
               RowBox[{"start", " ", "=", " ", "root"}], ";", "\n", 
               "\t\t\t\t", 
               RowBox[{"valueAtStart", " ", "=", " ", "rootValue"}], ";"}]}], 
             "\n", "\t\t\t", "]"}], ";"}]}], "\n", "\t\t", "]"}], ";"}]}], 
       "\t", "\n", "\t", "]"}], ";", "\n", "\t", 
      RowBox[{"End", "[", "]"}], ";", "\n", "\t", 
      RowBox[{"{", 
       RowBox[{"root", ",", " ", "bounds", ",", " ", "slopes"}], "}"}]}]}], 
    "\n", "]"}]}], ";"}]], "Code",
