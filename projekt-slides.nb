Cell[TextData[{
 StyleBox["f - funkcja, kt\[OAcute]rej miejsce zerowe nale\:017cy znale\:017a\
\[CAcute]\naround - punkt startowy, miejsce zerowe powinno znajdowa\[CAcute] \
si\:0119 w jego otoczeniu.\niterationsLimit - maksymalna dozwolona liczba \
iteracji\nminInterval - minimalna warto\:015b\[CAcute] jak\:0105 mo\:017ce \
przyj\:0105\[CAcute] |intervalStart - intervalEnd|\nprecision  - maksymalna \
zadowalaj\:0105ca nas odleg\[LSlash]o\:015b\[CAcute] od zera",
  FontSlant->"Italic"],
 "\n",
 StyleBox["newtonRawson zwraca tr\[OAcute]jk\:0119 (x, approximations, \
slopes), gdzie x jest obliczonym miejscem zerowym, approximations to kolejno \
obliczane jego przybli\:017cenia, a slopes to warto\:015bci pochodnej w tych \
punktach.",
  FontSlant->"Italic"]
}], "SideNote",
 CellChangeTimes->{
  3.946917030616778*^9},ExpressionUUID->"b4544500-3efc-4031-913a-\
82a711e53e2f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"newtonRawson", " ", "=", " ", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "f", ",", " ", "around", ",", " ", "iterationsLimit", ",", " ", 
       "minInterval", ",", " ", "precision"}], "}"}], ",", "\n", "\t", 
     RowBox[{
      RowBox[{"root", " ", "=", " ", "around"}], ";", "\n", "\t", 
      RowBox[{"approximations", " ", "=", " ", 
       RowBox[{"{", "}"}]}], ";", "\n", "\t", 
      RowBox[{"slopes", " ", "=", " ", 
       RowBox[{"{", "}"}]}], ";", "\n", "\t", 
      RowBox[{"Begin", "[", "\"\<newtonRawson`\>\"", "]"}], ";", " ", "\n", 
      "\t", 
      RowBox[{"midValue", " ", "=", " ", 
       RowBox[{"f", "[", "root", "]"}]}], ";", " ", "\n", "\t", 
      RowBox[{"width", " ", "=", " ", "minInterval"}], ";", "\n", "\t", 
      RowBox[{"df", " ", "=", " ", 
       RowBox[{
        RowBox[{"Derivative", "[", "1", "]"}], "[", "f", "]"}]}], ";", "\n", 
      "\t", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "0"}], ",", " ", 
        RowBox[{
         RowBox[{"i", " ", "<", " ", "iterationsLimit"}], " ", "&&", " ", 
         RowBox[{
          RowBox[{"Abs", "[", "width", "]"}], " ", ">=", " ", "minInterval"}],
          " ", "&&", " ", 
         RowBox[{
          RowBox[{"Abs", "[", "midValue", "]"}], " ", ">=", " ", 
          "precision"}]}], ",", " ", 
        RowBox[{"++", "i"}], ",", "\n", "\t\t", 
        RowBox[{
         RowBox[{"slope", " ", "=", " ", 
          RowBox[{"df", "[", "root", "]"}]}], ";", "\n", "\t\t", 
         RowBox[{"newRoot", " ", "=", " ", 
          RowBox[{"root", " ", "-", " ", 
           FractionBox["midValue", "slope"]}]}], ";", "\n", "\t\t", 
         RowBox[{"width", " ", "=", " ", 
          RowBox[{"Abs", "[", 
           RowBox[{"newRoot", " ", "-", " ", "root"}], "]"}]}], ";", "\n", 
         "\t\t", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"approximations", ",", " ", 
           RowBox[{"{", 
            RowBox[{"root", ",", " ", "newRoot"}], "}"}]}], "]"}], ";", "\n", 
         "\t\t", 
         RowBox[{"root", " ", "=", " ", "newRoot"}], ";", "\n", "\t\t", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"slopes", ",", " ", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"1", ",", " ", "slope"}], "}"}], ",", " ", 
             RowBox[{"{", 
              RowBox[{"0", ",", " ", "1"}], "}"}]}], "}"}]}], "]"}], ";", 
         "\n", "\t\t", 
         RowBox[{"midValue", " ", "=", " ", 
          RowBox[{"N", "[", 
           RowBox[{"f", "[", "root", "]"}], "]"}]}], ";"}]}], "\n", "\t", 
       "]"}], ";", "\n", "\t", 
      RowBox[{"End", "[", "]"}], ";", "\n", "\t", 
      RowBox[{"{", 
       RowBox[{"root", ",", " ", "approximations", ",", " ", "slopes"}], 
       "}"}]}]}], "\n", "]"}]}], ";"}]], "Code",
